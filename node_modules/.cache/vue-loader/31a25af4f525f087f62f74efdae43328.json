\" class=\"name-wrapper\">\r\n            <!-- <input type=\"text\"> -->\r\n            <el-tag size=\"medium\">{{ scope.row.name }}</el-tag>\r\n          </div>\r\n        </el-popover>\r\n      </template>\r\n    </el-table-column>\r\n    <!-- 第二列 -->\r\n        <el-table-column\r\n      label=\"日期\"\r\n      width=\"180\"\r\n      >\r\n      <template slot-scope=\"scope\" show-header=\"false\">\r\n        <i class=\"el-icon-time\"></i>\r\n        <span style=\"margin-left: 10px\">{{ scope.row.date }}</span>\r\n      </template>\r\n    </el-table-column>\r\n    <!-- 第三列 -->\r\n    <!-- 查看按钮 -->\r\n    <el-table-column label=\"操作\">\r\n      <template slot-scope=\"scope\">\r\n\r\n  <el-button type=\"text\" @click=\"dialogVisible = true\"  size=\"mini\"  plain style=\"margin-right: 10px\">查  看    \r\n          </el-button>\r\n\r\n<el-dialog\r\n  title=\"查看\"\r\n  :visible.sync=\"dialogVisible\"\r\n  width=\"30%\"\r\n  :before-close=\"handleClose\">\r\n  <span>{{ scope.row.address }}</span>\r\n            <div slot=\"reference\" class=\"name-wrapper\">\r\n            <el-tag size=\"medium\">{{ scope.row.name }}</el-tag>\r\n          </div>\r\n  <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"dialogVisible = false\" plain>确 定</el-button>\r\n  </span>\r\n</el-dialog>\r\n\r\n<!-- 编辑按钮 -->\r\n\r\n        <el-button type=\"text\" @click=\"open\" plain>编辑</el-button>\r\n\r\n          \r\n\r\n\r\n\r\n<!-- 删除按钮 -->\r\n        <el-button\r\n          size=\"mini\"\r\n          type=\"danger\"\r\n          @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n          \r\n      </template>\r\n      \r\n    </el-table-column>\r\n  </el-table>\r\n  <!-- 输入内容 -->\r\n  <textarea rows=\"10\" cols=\"30\" id=\"wbk\">\r\n我是一个文本框。\r\n</textarea>\r\n<el-input\r\n  type=\"textarea\"\r\n  :rows=\"2\"\r\n  placeholder=\"请输入内容\"\r\n  v-model=\"textarea\">\r\n</el-input>\r\n  </div>\r\n</template>\r\n\r\n  \r\n\r\n<script>\r\n// //对象\r\n// const info = {name: \"huo\", age: 18,id: \"1\"}\r\n// //字符串\r\n// const str = \"haha\";\r\n\r\n// localStorage.setItem('huo',JSON.stringify(info));\r\n\r\n// localStorage.setItem('zheng',str)\r\n\r\n  export default {\r\n    data() {\r\n      return {\r\n        textarea: ''\r\n      \r\n    }\r\n    \r\n    \r\n  }\r\n</script>\r\n<style scoped>\r\n#wbk{\r\n  float: left;\r\n}\r\n#di{\r\n  width:800px;\r\n  \r\n}\r\n</style>"]}]}